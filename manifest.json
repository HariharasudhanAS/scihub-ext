{

  "manifest_version": 2,
  "name": "sci-hub",
  "version": "0.1",

  "description": "Redirects to https://www.sci-hub.tw/<url> on click",
  "homepage_url": "https://github.com/HariharasudhanAS/sci-hubEXT",
  "icons": {
    "96": "icons/sci-hub.png"
  },

  "permissions": [
    "tabs"
  ],

  "browser_action": {
    "default_icon": "icons/sci-hub.png",
    "default_title": "Sci-hub"
  },

  "background": {
     "scripts": ["background.js"]
  }


}
